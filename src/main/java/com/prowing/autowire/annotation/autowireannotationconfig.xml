<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">
	<context:annotation-config />
	<bean name="address2"
		class=" com.prowing.autowire.annotation.Address">
		<property name="street" value="Satatana road"></property>
		<property name="city" value="Satatna">
		</property>

	</bean>
	<bean name="address1"
		class=" com.prowing.autowire.annotation.Address">
		<property name="street" value="soygaon road"></property>
		<property name="city" value="malegaon">
		</property>

	</bean>
	
	<bean name="employee"
		class="com.prowing.autowire.annotation.Employee"></bean>

</beans> 